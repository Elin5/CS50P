name = input("What's your name?")

if name == "Harry":
    print("Griffindor")
elif  name == "Hermione":
    print("Griffindor")
elif  name == "Ron":
    print("Griffindor")
elif  name == "Draco":
    print("Slytherin")
else:
    print("Who?")
