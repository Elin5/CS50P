for i in range(n):
    print("üêë" * i)
#
#
def main():
    n = int(input("What's n? "))
    for i in range(n):
        print("üêë" * i)


if __name__ == "__main__":
    main()
#
#
def main():
    n = int(input("What's n? "))
    for i in range(n):
        print(sheep(i))


def sheep(n):
    return "üêë" * n


if __name__ == "__main__":
    main()
#
#
def main():
    n = int(input("What's n? "))
    for s in sheep(n):
        print(s)


def sheep(n):
    flock = []
    for i in range(n):
        flock.append("üêë" * i)
    return flock


if __name__ == "__main__":
    main()
#
#
def main():
    n = int(input("What's n? "))
    for s in sheep(n):
        print(s)


def sheep(n):
    for i in range(n):
        yield "üêë" * i


if __name__ == "__main__":
    main()
